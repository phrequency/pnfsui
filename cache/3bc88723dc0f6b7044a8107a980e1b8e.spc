  ";s:7:"attribs";a:0:{}s:8:"xml_base";s:33:"http://www.ocfrealty.com/ocf-news";s:17:"xml_base_explicit";b:1;s:8:"xml_lang";s:0:"";s:5:"child";a:2:{s:0:"";a:5:{s:5:"title";a:1:{i:0;a:5:{s:4:"data";s:60:"Apartment Building Further Improves an East Kensington Block";s:7:"attribs";a:0:{}s:8:"xml_base";s:33:"http://www.ocfrealty.com/ocf-news";s:17:"xml_base_explicit";b:1;s:8:"xml_lang";s:0:"";}}s:4:"link";a:1:{i:0;a:5:{s:4:"data";s:114:"http://www.ocfrealty.com/naked-philly/east-kensington/apartment-building-further-improves-an-east-kensington-block";s:7:"attribs";a:0:{}s:8:"xml_base";s:33:"http://www.ocfrealty.com/ocf-news";s:17:"xml_base_explicit";b:1;s:8:"xml_lang";s:0:"";}}s:11:"description";a:1:{i:0;a:5:{s:4:"data";s:6333:"<div class="field field-name-field-tagline field-type-text field-label-hidden"><div class="field-items"><div class="field-item even">Joins another apartment building</div></div></div><div class="field field-name-field-author field-type-text field-label-hidden"><div class="field-items"><div class="field-item even">Mr. Fox</div></div></div><div class="field field-name-share-buttons-v2 field-type-ds field-label-hidden"><div class="field-items"><div class="field-item even"><div class="addthis_toolbox addthis_default_style " addthis:url="http://www.ocfrealty.com/naked-philly/east-kensington/apartment-building-further-improves-an-east-kensington-block" addthis:title="Apartment Building Further Improves an East Kensington Block" addthis:description="">
<a class="addthis_button_facebook_like" fb:like:layout="button_count"></a>
